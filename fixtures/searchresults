					    {{#Brokers}}
						    <a class="fsbo_broker" href="{{ProfileUrl}}" title="{{{Name}}}" data-account="{{Account}}" data-actiontype="{{index}}">
							    <img src="{{ImageSource}}" alt="{{{Name}}}">
							    <span class="fsbo_brokerName">{{{Name}}}</span>
							    <span class="fsbo_brokerCompany">{{{CompanyName}}}</span>
							    <span class="fsbo_brokerLocation">{{Location}}</span>
							    </a>
					    {{/Brokers}}
					    {{#MoreBrokersLink}}
					    <p>
						    <a href="{{MoreBrokersLink}}" title="More {{RegionName}} Brokers" class="fsbo_moreBrokers">More {{RegionName}} Brokers</a>
					    </p>
					    {{/MoreBrokersLink}}
				    {{/BrokerSearchResult}}
                </script>
                <script type="text/javascript">
                    $(function()
                    {
                        var BizBuySellWebSite = window.BizBuySellWebSite;
                        try
                        {
                            if ((BizBuySellWebSite) &&
                            (BizBuySellWebSite.services) &&
                            (BizBuySellWebSite.services.DataUtilities) &&
                            (BizBuySellWebSite.services.DataUtilities.GetBrokerSearchResultModel))
                            {
                                setTimeout(function()
                                    {
                                        BizBuySellWebSite.services.DataUtilities.GetBrokerSearchResultModel(
                                            '241da481-6e59-4b33-96ac-f96c30c747da',
                                            '<?xml version="1.0" encoding="utf-16"?>  <ListingSearchCriteria xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">    <SearchCriteriaQueryStringData>cffrom=200000&amp;cfto=500000&amp;s=NY&amp;ir=1&amp;spid=3&amp;tab=eb&amp;lsr_rlbpn=2</SearchCriteriaQueryStringData>    <HtmlEncodedSearchCriteriaQueryStringData>cffrom=200000&amp;cfto=500000&amp;s=NY&amp;ir=1&amp;spid=3&amp;tab=eb&amp;lsr_rlbpn=2</HtmlEncodedSearchCriteriaQueryStringData>    <CriteriaQueryStringParts>      <item>        <key>          <string>cffrom</string>        </key>        <value>          <string>200000</string>        </value>      </item>      <item>        <key>          <string>cfto</string>        </key>        <value>          <string>500000</string>        </value>      </item>      <item>        <key>          <string>s</string>        </key>        <value>          <string>NY</string>        </value>      </item>      <item>        <key>          <string>tab</string>        </key>        <value>          <string>eb</string>        </value>      </item>      <item>        <key>          <string>lsr_rlbpn</string>        </key>        <value>          <string>2</string>        </value>      </item>    </CriteriaQueryStringParts>    <CriteriaInitialized>true</CriteriaInitialized>    <SearchType>EstablishedBusinesses</SearchType>    <Regions>      <Region>        <RegionID>48</RegionID>        <Description>New York</Description>        <CountryID>1</CountryID>        <Abbreviation>NY</Abbreviation>        <LegacyCode>NY</LegacyCode>        <SpecialRegionID>8</SpecialRegionID>        <SpecialInternationalRegionID>0</SpecialInternationalRegionID>        <ItemID>48</ItemID>      </Region>    </Regions>    <CashFlow>      <MaxValue>500000</MaxValue>      <MinValue>200000</MinValue>    </CashFlow>    <KeywordMatchType>Any</KeywordMatchType>    <DoNotShowListingsWithUndisclosedPrices>false</DoNotShowListingsWithUndisclosedPrices>    <OnlyShowListingsWithSellerFinancing>false</OnlyShowListingsWithSellerFinancing>    <OnlyShowListingsWithRealEstateIncluded>false</OnlyShowListingsWithRealEstateIncluded>    <ShowRelocatableListings>false</ShowRelocatableListings>    <DateListed>0001-01-01T00:00:00</DateListed>    <DateFirstApproved>0001-01-01T00:00:00</DateFirstApproved>    <DateListedType>Anytime</DateListedType>    <HomeBasedType>Include</HomeBasedType>    <AskingPriceType>None</AskingPriceType>    <ExcludeNoAskingPrice>false</ExcludeNoAskingPrice>    <IsInitialSearchRequest>false</IsInitialSearchRequest>    <SearchStarted>false</SearchStarted>    <SortFields>      <item>        <key>          <enBizopsSortFields>Default</enBizopsSortFields>        </key>        <value>          <enumSequence>Desc</enumSequence>        </value>      </item>    </SortFields>    <IsSpecialInternationalRegion>false</IsSpecialInternationalRegion>    <CurrentPageNumber>3</CurrentPageNumber>    <SearchOriginID>0</SearchOriginID>    <IsReturnToSearch>false</IsReturnToSearch>    <WhoFrom>0</WhoFrom>    <ExcludeAssetSales>false</ExcludeAssetSales>    <IsBotSearch>false</IsBotSearch>    <BrokerAccount>0</BrokerAccount>    <ReinforcementListingsBeginningPageNumber>2</ReinforcementListingsBeginningPageNumber>  </ListingSearchCriteria>',
                                            function(results)
                                            {
                                                if ((results != null) &&
                                                (results.hasOwnProperty('d')))
                                                {
                                                    results = results.d;
                                                }

                                                var fsbo_brokerSearch_content = $('#fsbo_brokerSearch_content');

                                                if (fsbo_brokerSearch_content.length > 0)
                                                {
                                                    fsbo_brokerSearch_content.empty();

                                                    if (window.Mustache)
                                                    {
                                                        var fsbo_brokerSearch_template = Mustache.compile(
                                                            $('#fsbo_brokerSearch_template').html());

                                                        if (typeof(fsbo_brokerSearch_template) == 'function')
                                                        {
                                                            //Action type for attribute for Right Rail Brokers
                                                            var brokerIndex = 40;

                                                            fsbo_brokerSearch_content
                                                                .prepend(
                                                                    fsbo_brokerSearch_template(
                                                                    {
                                                                        BrokerSearchResult: results,
                                                                        'index': function() {
                                                                            return brokerIndex += 20;
                                                                        }
                                                                    }));
                                                        }
                                                    }
                                                }

                                                $('a[data-account]').each(function()
                                                {
                                                    var account = $(this).attr('data-account');
                                                    var actionType = $(this).attr('data-actiontype');
                                                    $(this).executeBeforeNavigation(
                                                        function(navigate)
                                                        {
                                                            navigate = navigate || function() {};
                                                            if ((window.BizBuySellWebSite) &&
                                                            (window.BizBuySellWebSite.services) &&
                                                            (window.BizBuySellWebSite.services.DataUtilities) &&
                                                            (window.BizBuySellWebSite.services.DataUtilities.TrackBrokerEvent))
                                                            {
                                                                account = (account || 0);
                                                                actionType = (actionType || 0);

                                                                if ((account > 0) &&
                                                                (actionType > 0))
                                                                {
                                                                    window.BizBuySellWebSite.services.DataUtilities.TrackBrokerEvent(
                                                                        account,
                                                                        actionType);
                                                                }
                                                            }

                                                            navigate();
                                                        });
                                                });

                                                if (results != null)
                                                {
                                                    for (var index = 0; index < results.Brokers.length; index++)
                                                    {
                                                        ga('send', 'event', 'listing_search_results', 'broker_directory_upsell_multi_display', results.Brokers[index].BrokerProfileID);
                                                    }
                                                }
                                            });
                                    },
                                    1);
                            }

                        }
                        catch(err)
                        {
                        }
                    });
                </script>
