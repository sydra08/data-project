    <script type="text/javascript">
	    $(function()
	    {
	        if ((window.CommunityQnA) &&
	            (typeof(CommunityQnA.SetRegistrationHyperLinkClientID) == 'function'))
	        {
	            CommunityQnA.SetRegistrationHyperLinkClientID(
	                'hlJoinMyBBS');
	        }

	        $('#LoginModal').on('shown', function()
	        {
	            $('#txtUserName').focus();
	        });
		    
	    });

	    var validationControls =
	    [
		    {
			    ClientID: 'txtFirstName',
			    RequiredField: false,
			    CustomValidationFunction: function(field)
			    {
				    var isValid = true;

				    if (field.val() == '')
				    {
					    isValid = false;

					    $('#plcInvalidFirstName').show();
				    }
				    else
				    {
				    	$('#plcInvalidFirstName').hide();
				    }

				    return isValid;
			    }
		    },
		    {
		    	ClientID: 'txtLastName',
		    	RequiredField: false,
		    	CustomValidationFunction: function (field)
		    	{
		    		var isValid = true;

		    		if (field.val() == '')
		    		{
		    			isValid = false;

		    			$('#plcInvalidLastName').show();
		    		}
		    		else
		    		{
		    			$('#plcInvalidLastName').hide();
		    		}

		    		return isValid;
		    	}
		    },
		    {
		    	ClientID: 'txtCreateUserName',
		    	RequiredField: false,
		    	CustomValidationFunction: function (field)
		    	{
		    	    var rRegularExpressionPattern = new RegExp("^[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?\\.)+[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?$", 'g');

		    		var isValid = true;

			        if ((field.val() == '') ||
			            ((field.val() != '') && (!rRegularExpressionPattern.test(field.val()))))
			        {
			            isValid = false;
			            $('#plcInvalidContactEmail').show();
                    }
                    else
			        {
			            $('#plcInvalidContactEmail').hide();
			        }

			        return isValid;
		    	}
		    },
		    {
		    	ClientID: 'txtCreatePassword',
		    	RequiredField: false,
		    	CustomValidationFunction: function (field)
		    	{
		    		var isValid = true;

		    		if (field.val() == '' || field.val().length < 6)
		    		{
		    			isValid = false;

		    			$('#plcInvalidPassword').show();
		    		}
		    		else
		    		{
		    			$('#plcInvalidPassword').hide();
		    		}

		    		return isValid;
		    	}
            }
            
	    ];

    	var isValid = true;
    	var personEmailAddressIsValidForNewUser;

    	$('#txtFirstName, #txtLastName, #txtCreatePassword').blur(function()
    	{
    		if ($(this).val() == '')
    		{
    			$(this).addClass('error');
    			$(this).parent().parent().addClass('error');
    		}
    		else
    		{
    			$(this).removeClass('error');
    			$(this).parent().parent().removeClass('error');
    		}
        });

        
    	
    	$('#txtCreateUserName').blur(function()
    	{
    		if ($(this).val() == '')
    		{
    			$(this).addClass('error');
    			$(this).parent().parent().addClass('error');
    		}
    		else
    		{
    			$.ajax({
    				url: '/users/Services/Users.asmx/IsPersonEmailValidForNewUser',
    				type: 'POST',
    				async: false,
    				dataType: 'json',
    				contentType: 'application/json; charset=utf-8',
    				data: JSON.stringify(
						{
							emailAddress: $('#txtCreateUserName').val(),
							password: '',
							registrationType: 0
						}),
    				success: function (
						result)
    				{
    					result = (result || { d: { IsValid: false, IsExistingUser: false} });

    					if (result.hasOwnProperty('d'))
    					{
    						result = result.d;
    					}
    					
    					personEmailAddressIsValidForNewUser = result.IsValid;
    				},
    				error: function ()
    				{
    					personEmailAddressIsValidForNewUser = false;
    				}
    			});
    		
    			if (personEmailAddressIsValidForNewUser)
    			{
    				$('#plcInvalidExistingEmail').hide();
    				if (isValid)
    				{
    					$('#divCreateAccountValidation').hide();
    				}
    				$(this).removeClass('error');
    				$(this).parent().parent().removeClass('error');
    			}
    			else
    			{
    				$(this).addClass('error');
    				$(this).parent().parent().addClass('error');
    				$('#plcInvalidExistingEmail').show();
    				$('#divCreateAccountValidation').show();
    			}
    		}
    	});

	    $('#hlModalSignup').click(function()
	    {
	    	isValid = Validation.ManualValidate(
                validationControls);
		    
	    	if (isValid && personEmailAddressIsValidForNewUser)
	    	{
	    		$('#divCreateAccountValidation').hide();

	    		var newsletterType = 0;

	    		if ($('#chkBuyerNewsletter').prop('checked'))
			    {
			        newsletterType = 40;
	    		}
	    		else if ($('#chkBrokerNewsletter').prop('checked'))
	    		{
	    		    newsletterType = 20;
	    		}
	    		else if ($('#chkFranchiseNewsletter').prop('checked'))
	    		{
	    		    newsletterType = 30;
	    		}
	    		else if ($('#chkSellerNewsletter').prop('checked'))
	    		{
	    		    newsletterType = 60;
	    		}

            
	    		MyBBS.CreateUserAndLogin(
	    			$('#txtFirstName').val(),
	    			$('#txtLastName').val(),
	    			$('#txtCreateUserName').val(),
	    			$('#txtCreatePassword').val(),
	    			'',
	    			newsletterType,
                    0);
                
	    	}
	    	else
	    	{
	    		$('#divCreateAccountValidation').show();
	    	}
	    });

	    $('#hlMemberSignIn').click(function()
	    {
	    	$('#LoginModal').removeClass('createaccount');
	    });

	    $('#spnCreateNewsletter').click(function()
	    {
	    	$('#chkCreateNewsletter').trigger('click');
	    });

	    $('#hlJoinMyBBS').click(function()
	    {
	    	$('#divDisableMyBBSLoginControls').css('display', '');
	    	if ($('#plcLoginInvalidData').css('display') != 'none' || $('#plcLoginInvalidLogin').css('display') != 'none')
	    	{
	    		$('#divValidation').css('display', '');
	    	}
	    });
	</script>
	 <script type="text/javascript" src="https://www.bizbuysell.com/xcommon/handlers/compression/scripts.ashx?n=/xcommon/scripts/Validation.js&amp;v=883897858">

</script>
</div>    


        
    </body>
</html>