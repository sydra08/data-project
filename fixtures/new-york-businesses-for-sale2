                        '<?xml version="1.0" encoding="utf-16"?>  <CoBrand xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true" />',
                        function(results)
                        {
                            if (results.hasOwnProperty('d'))
                            {
                                results = results.d;
                            }
                                                        
                            if (results.length > 0)
                            {
                                $('#divFranchiseSearch').show();
                            }

                            for (var index = 0; index < results.length; index++)
                            {
                                $('#FranchiseRowContainer').append(
                                    Mustache.render($('#Franchises_template').text(), results[index]));

                                $('#relatedFranchise' + results[index].FranchiseID).attr('rel-fd-click-id', results[index].FranchiseID);

                                $('#relatedFranchise' + results[index].FranchiseID).attr('rel-fd-click-event', results[index].ClickTypeEvent);
                            }
                                                    

                            $('a[rel-fd-click-id]').each(function()
                            {
                                var fid = $(this).attr('rel-fd-click-id');
                                var faction = $(this).attr('rel-fd-click-event');
                                $(this).executeBeforeNavigation(
                                    function(navigate)
                                    {
                                        window.TrackFranchiseClickEvent(fid, faction, (navigate || function() {}));
                                    });
                            });
                        });
                }, 1);
            }
        }
    }
    catch(err)
    {
    }
</script>
