<script type="text/javascript">
    $('#hlContactBrokerModal_23202').click(function()
    {
        var isValid = true;

        $('#brokerContactModalBody_23202 input.validate')
            .each(function()
            {
                var $this = $(this);
                isValid = validate23202($this) && isValid;
            });

        $('#brokerContactModalBody_23202 select')
            .each(function()
            {
                var $this = $(this);
                isValid = validate23202($this) && isValid;
            });

        if (isValid) 
        {
            
            $('#contactBrokerButton')
                .addClass('disabled')
                .addClass('contacted')
                .text('Broker Contacted')
                .unbind('click');
            
            $('#contactBrokerModal').modal('hide');
            

            var role = $('#ctl00_Content_ucSearchResultsContent_ContactBroker_ddlContactTypeBrokerModal').val();
            var txtEmail = $('#txtEmailBrokerModal_23202').val();
            var txtPhone = $('#txtPhoneBrokerModal_23202').val();
            var txtName = $('#txtNameBrokerModal_23202').val();
            var txtMessage = $('#txtMessageModal_23202').val() || '';

            window.BizBuySellWebSite.brokerdirectory.Services.BrokerDirectoryUtilities.ContactBroker(
                30236,
                txtName,
                txtEmail,
                txtPhone,
                role,
                txtMessage,
                'd');

            ga('send', 'event', 'listing_search_results', 'contactbroker', 'sent');
            ga('send', 'event', 'broker_directory', 'contactbroker', 'sent');

            if (window.BizBuySellWebSite &&
                window.BizBuySellWebSite.services &&
                window.BizBuySellWebSite.services.DataUtilities &&
                window.BizBuySellWebSite.services.DataUtilities.TrackBrokerEvent)
            {
                window.BizBuySellWebSite.services.DataUtilities.TrackBrokerEvent(
                    30236,
                    510);
            }
        }
    });
</script>

<script type="text/javascript">
    $('.contactForm input').focus(function()
    {
        $(this).removeClass('error');
    });

    $('.contactForm input').blur(function()
    {
        var $this = $(this);
        validate23202($this);
    });

    $('.contactForm select').focus(function()
    {
        $(this).removeClass('error');
    });

    $('.contactForm select').blur(function()
    {
        var $this = $(this);
        validate23202($this);
    });

    var validate23202 = function($this)
    {
        var isValid = true;

        if ($this.val().length == 0)
        {
            $this.addClass('error');
            isValid = false;
        }
        else if ($this.attr('id') == 'ctl00_Content_ucSearchResultsContent_ContactBroker_txtEmail' || $this.attr('id') == 'txtEmailBrokerModal_23202' )
        {
            var emailRegEx = new RegExp("^[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?\\.)+[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?$", 'g');
                
            if (!emailRegEx.test($this.val()))
            {
                $this.addClass('error');
                isValid = false;
            }
        }
        else if ($this.attr('id') == 'ctl00_Content_ucSearchResultsContent_ContactBroker_txtPhone' || $this.attr('id') == 'txtPhoneBrokerModal_23202')
        {
            var phoneRegEx = new RegExp("(^1?[- .]?(\\([2-9]\\d{2}\\)|[2-9]\\d{2})[- .]?(\\d{3})[- .]?(\\d{4})(\\s*e?[Xx]t?(ension)?[.:]*\\s*\\d{1,5})?$|^\\+?\\d{1,3}[ -]?[0-9 -]{9,14}(\\s*e?[Xx]t?(ension)?[.:]*\\s*\\d{1,5})?$)", 'gi');

            if (!phoneRegEx.test($this.val()))
            {
                $this.addClass('error');
                isValid = false;
            }
        }
        else if ($this.attr('id') == 'ctl00_Content_ucSearchResultsContent_ContactBroker_ddlContactType' || $this.attr('id') == 'ctl00_Content_ucSearchResultsContent_ContactBroker_ddlContactTypeBrokerModal')
        {
            if ($this.val() == "0")
            {
                $this.addClass('error');
                isValid = false;
            }
        }

        return isValid;
    };
</script>

</div>

