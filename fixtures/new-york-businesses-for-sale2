                        '<?xml version="1.0" encoding="utf-16"?>  <ListingSearchCriteria xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">    <SearchCriteriaQueryStringData>s=NY&amp;tab=eb</SearchCriteriaQueryStringData>    <HtmlEncodedSearchCriteriaQueryStringData>s=NY&amp;tab=eb</HtmlEncodedSearchCriteriaQueryStringData>    <CriteriaQueryStringParts>      <item>        <key>          <string>s</string>        </key>        <value>          <string>NY</string>        </value>      </item>      <item>        <key>          <string>tab</string>        </key>        <value>          <string>eb</string>        </value>      </item>    </CriteriaQueryStringParts>    <CriteriaInitialized>true</CriteriaInitialized>    <SearchType>EstablishedBusinesses</SearchType>    <Regions>      <Region>        <RegionID>48</RegionID>        <Description>New York</Description>        <CountryID>1</CountryID>        <Abbreviation>NY</Abbreviation>        <LegacyCode>NY</LegacyCode>        <SpecialRegionID>8</SpecialRegionID>        <SpecialInternationalRegionID>0</SpecialInternationalRegionID>        <ItemID>48</ItemID>      </Region>    </Regions>    <KeywordMatchType>Any</KeywordMatchType>    <DoNotShowListingsWithUndisclosedPrices>false</DoNotShowListingsWithUndisclosedPrices>    <OnlyShowListingsWithSellerFinancing>false</OnlyShowListingsWithSellerFinancing>    <OnlyShowListingsWithRealEstateIncluded>false</OnlyShowListingsWithRealEstateIncluded>    <ShowRelocatableListings>false</ShowRelocatableListings>    <DateListed>0001-01-01T00:00:00</DateListed>    <DateFirstApproved>0001-01-01T00:00:00</DateFirstApproved>    <DateListedType>Anytime</DateListedType>    <HomeBasedType>Include</HomeBasedType>    <AskingPriceType>None</AskingPriceType>    <ExcludeNoAskingPrice>false</ExcludeNoAskingPrice>    <IsInitialSearchRequest>false</IsInitialSearchRequest>    <SearchStarted>true</SearchStarted>    <SortFields>      <item>        <key>      