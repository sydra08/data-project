    <script type="text/javascript">
        var PMPopupSeen = ($.cookie('PMPopupSeenCookie') || '');
        var AnyModalSeen = ($.cookie('AnyModalSeenCookie') || '');
        var ga;

        if ((($.cookie('showPMPopupCookie') == '1') || 1 >= 2 ) &&
        (PMPopupSeen == '0' || PMPopupSeen == '') && (AnyModalSeen == '' || AnyModalSeen == '0'))
        {
            $('#PremMemberModal').modal('show');
                
            (function()
            {
                ga('send', 'event', 'listing_search_results', 'premmember_popup', 'show');
            })();
                
            $.cookie(
                'showPMPopupCookie', 
                '0',
                {
                    path: '/',
                    domain: '.bizbuysell.com'
                });
            $.cookie(
                'PMPopupSeenCookie', 
                '1',
                {
                    expires: 30,
                    path: '/',
                    domain: '.bizbuysell.com'
                });
        }
    </script>
    
