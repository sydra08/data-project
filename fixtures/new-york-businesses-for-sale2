<script type="text/javascript">
	$('.CategoryCheckBox').click(function(e)
	{
		e.stopPropagation();
	});
	$('#CategoryModal button').click(function()
	{
		$('#CategoryModal').modal('hide');
	});
	$('.checktoggle').click(function(e)
	{
		e.stopPropagation();
		e.preventDefault();
		var x = $(this).attr('href');
		var checkBoxes = $(x + " :input");
	    var isChecked = $(this).text().indexOf('De') >= 0;

	    checkBoxes.prop('checked', !isChecked);
	    if (!isChecked)
		{
		    $(this).html('Deselect all');
		}
		else
		{
		    $(this).html('Select all');
		}

		if (x == '#ALLCATEGORIES')
		{
		    //Need to change all the subheadings
		    if (!isChecked)
		    {
		        $('#ALLCATEGORIES .checktoggle').html('Deselect all');
		    }
		    else
		    {
		        $('#ALLCATEGORIES .checktoggle').html('Select all');
		    }
		}
	});
</script>
		<div class="modal-footer">
			<button type="button" class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
			<button type="button" class="btn btn-primary" data-dismiss="modal" onclick="onCategoryButtonSave()">Save changes</button>
		</div>
	</div>

<script type="text/javascript">
    var StateButtonSaveEvent = false;

    function onStateButtonSave()
    {
        StateButtonSaveEvent = true;

        if (window.Listings && (typeof(Listings.LoadCountiesByRegionAbbreviationNew) == 'function'))
        {
            var sSelectedCountyCodes = '';
            var SelectedCountyCodes = sSelectedCountyCodes.split(',');
            Listings.LoadCountiesByRegionAbbreviationNew('input[name="s"]', 'ddlCounties', SelectedCountyCodes);
        }

        $('input.StateCheckBox').attr('data-checked', 'no');
        $('input.StateCheckBox:checked').attr('data-checked', 'yes');
    }

    var CountyButtonSaveEvent = false;

    function onCountyButtonSave()
    {
        CountyButtonSaveEvent = true;

        $('input.CountyCheckbox').attr('data-checked', 'no');
        $('input.CountyCheckbox:checked').attr('data-checked', 'yes');
    }

    var CategoryButtonSaveEvent = false;

    function onCategoryButtonSave()
    {
        CategoryButtonSaveEvent = true;

        $('input.CategoryCheckBox').attr('data-checked', 'no');
        $('input.CategoryCheckBox:checked').attr('data-checked', 'yes');
    }

    $(function()
    {
        $('.searchMenu *,#StateModal *,#CategoryModal *,#CountyModal *')
            .click(function(e)
            {
                e.stopPropagation();
            });

        $('.searchMenu a').click(function(e)
        {
            e.stopPropagation();
            var x = $(this).attr('href');
            $(x).modal('show');
        });

        $('#StateModal button').click(function()
        {
            $('#StateModal').modal('hide');
        });

        $('#StateModal').on('hide', function()
        {
            if (!StateButtonSaveEvent)
            {
                $('input.StateCheckBox[data-checked=yes]').prop('checked', true);
                $('input.StateCheckBox[data-checked=no]').prop('checked', false);
            }
            StateButtonSaveEvent = false;
        });

        $('#CountyModal button').click(function()
        {
            $('#CountyModal').modal('hide');
        });

        $('#CountyModal').on('hide', function()
        {
            if (!CountyButtonSaveEvent)
            {
                $('input.CountyCheckbox[data-checked=yes]').prop('checked', true);
                $('input.CountyCheckbox[data-checked=no]').prop('checked', false);
            }
            CountyButtonSaveEvent = false;
        });

        $('#CategoryModal button').click(function()
        {
            $('#CategoryModal').modal('hide');
        });

        $('#CategoryModal').on('hide', function()
        {
            if (!CategoryButtonSaveEvent)
            {
                $('input.CategoryCheckBox[data-checked=yes]').prop('checked', true);
                $('input.CategoryCheckBox[data-checked=no]').prop('checked', false);
            }
            CategoryButtonSaveEvent = false;
        });

        onStateButtonSave();

        $('#allusstate').change(function()
        {
            if ($(this).is(':checked'))
            {
                $('.localregion').children('input').prop('checked', true);
                $('.checktoggleState').html('Deselect all');
            }
            else
            {
                $('.localregion').children('input').prop('checked', false);
                $('.checktoggleState').html('Select all');
            }
        });

        $('.checktoggleState').click(function(e)
        {
            e.stopPropagation();
            e.preventDefault();
            var checkBoxes = $('.StateCheckBox');
            checkBoxes.prop('checked', !checkBoxes.prop('checked'));
            if ('Select all' == $(this).html())
            {
                $(this).html('Deselect all');
            }
            else
            {
                $(this).html('Select all');
            }
        });

    });

    $('#myModal').on('hidden', function()
    {
        $('#refine').trigger('click');
    });

    $('#BMclick').trigger('click');

    $('#lessSearch').hide();

    $('#moreSearch').on('click', function(e)
    {
        e.stopPropagation();
        $('#moreSearch').fadeOut('fast');
        $('.searchMenu').animate({ width: '700px' }, 'fast', function()
        {
            $('#moreOptsC1').fadeIn().css("display", "inline-block");
            $('#moreOptsC2').fadeIn().css("display", "inline-block");

            $('#lessSearch').show();
        });

    });

    $('#lessSearch').on('click', function(e)
    {
        e.stopPropagation();
        $('#moreOptsC1').fadeOut('fast');
        $('#moreOptsC2').fadeOut('fast', function()
        {
            $('.searchMenu').animate({ width: '220px' });

            $('#lessSearch').hide();
            $('#moreSearch').show();
        });

    });
</script>

