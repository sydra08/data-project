    <script type="text/javascript">
        var showBizAlert = function()
        {
            $('#bizAlert_Save').hide();
            $('#bizAlert_Saved').show();
            $('#BizAlertModal').modal('hide');
        };

        var bizAlert = {
            OnSuccess: function()
            {
                showBizAlert();
            },
            OnError: function(bizAlertName)
            {
                console.log('Error saving BizAlert "' + bizAlertName + '"');
            },
            OnSave: function()
            {
                showBizAlert();
            },
            DisplaySuccess: function()
            {
                showBizAlert();
            }
        };

        var showSavedSearch = function()
        {
            $('#lableSavedSearch').show();
            $('#linkSaveSearchModal').hide();
            $('#BizAlertModal').modal('hide');
        };

        var savedSearch = {
            OnSuccess: function()
            {
                showSavedSearch();
            },
            OnError: function(searchName)
            {
                console.log('Error saving search "' + searchName + '"');
            },
            OnSave: function()
            {
                showSavedSearch();
            },
            DisplaySuccess: function()
            {
                showSavedSearch();
            }
        };

        InitBizAlert(
            {"BizAlert":{"Status":0,"AvailabilityTypes":null,"Source":0,"Name":null,"DeliverySchedule":10,"IsActive":true,"BotID":-2147483648,"ListNumber":-2147483648,"Account":0,"Email":"","States":"NY","SIC":"XX","Date":"","Heading":"","Summary":"","Keywords":null,"RecordType":null,"FlagMatchAll":null,"MatchAll":false,"FlagSize":null,"FlagEHome":null,"FlagRelocate":null,"FlagAsking":null,"FlagEmail":"n","FlagReview":null,"WhoFrom":null,"Flag1":null,"Flag2":null,"FName":"","LName":"","CoName":"","CoAddress":"","CoCity":"","CoState":null,"CoZip":"","Telephone":"","Role":null,"HowHear":null,"AmountDown":"","TimeFrame":"","CountyCodesList":null,"CountyCodes":null,"PersonID":0,"IsNetworkBlacklisted":false,"PrimaryKey":"-2147483648","ItemID":-2147483648},"BizAlertName":"New York","ExistingBotListNumber":0,"SubscribedToEmailAlert":false,"EmailAlertFrequency":0,"OptedInToBuyerBlast":false,"SearchSaved":false,"SavedSearchExists":false,"SearchName":"New York Businesses","SavedSearchCriteria":"s=NY\u0026tab=eb","OnRegistrationSuccessReturnUrl":"/listings/Search.aspx?s*NY^tab*eb^l_ssn*","NeedToSaveSearch":false,"BuyerBlastOptIn":false,"BizAlertExists":false,"NeedToSubscribeToBizAlert":false,"DisplayOnPageLoad":true},
