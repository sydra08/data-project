    var normalizePhotos = function()
    {
        $('.thumb:not(.noimage)').each(function()
        {
            var t = new Image();
            t.src = $(this).css('background-image').replace(/(?:url\(['"]?)?([^'")]+)['"]?\)?/, '$1');

            // Removing per Steve. We should always be using "contain" which is defined in searchresultsnew.css -BT
            //$(this).css('background-size', t.width / t.height < 1.35 ? 'contain' : 'cover');
        });
    };
</script>

