    <script type="text/javascript">
        var showBizAlert = function()
        {
            $('#bizAlert_Save').hide();
            $('#bizAlert_Saved').show();
            $('#BizAlertModal').modal('hide');
        };

        var bizAlert = {
            OnSuccess: function()
            {
                showBizAlert();
            },
            OnError: function(bizAlertName)
            {
                console.log('Error saving BizAlert "' + bizAlertName + '"');
            },
            OnSave: function()
            {
                showBizAlert();
            },
            DisplaySuccess: function()
            {
                showBizAlert();
            }
        };

        var showSavedSearch = function()
        {
            $('#lableSavedSearch').show();
            $('#linkSaveSearchModal').hide();
            $('#BizAlertModal').modal('hide');
        };

        var savedSearch = {
            OnSuccess: function()
            {
                showSavedSearch();
            },
            OnError: function(searchName)
            {
                console.log('Error saving search "' + searchName + '"');
            },
            OnSave: function()
            {
                showSavedSearch();
            },
            DisplaySuccess: function()
            {
                showSavedSearch();
            }
        };

        InitBizAlert(
            {"BizAlert":{"Status":0,"AvailabilityTypes":null,"Source":0,"Name":null,"DeliverySchedule":10,"IsActive":true,"BotID":-2147483648,"ListNumber":-2147483648,"Account":0,"Email":"","States":"XX","SIC":"R6","Date":"","Heading":"","Summary":"","Keywords":null,"RecordType":null,"FlagMatchAll":null,"MatchAll":false,"FlagSize":null,"FlagEHome":null,"FlagRelocate":null,"FlagAsking":null,"FlagEmail":"n","FlagReview":null,"WhoFrom":null,"Flag1":null,"Flag2":null,"FName":"","LName":"","CoName":"","CoAddress":"","CoCity":"","CoState":null,"CoZip":"","Telephone":"","Role":null,"HowHear":null,"AmountDown":"","TimeFrame":"","CountyCodesList":null,"CountyCodes":null,"PersonID":0,"IsNetworkBlacklisted":false,"PrimaryKey":"-2147483648","ItemID":-2147483648},"BizAlertName":"Software","ExistingBotListNumber":0,"SubscribedToEmailAlert":false,"EmailAlertFrequency":0,"OptedInToBuyerBlast":false,"SearchSaved":false,"SavedSearchExists":false,"SearchName":"Software","SavedSearchCriteria":"i=R6\u0026tab=eb","OnRegistrationSuccessReturnUrl":"/listings/Search.aspx?i*R6^tab*eb^l_ssn*","NeedToSaveSearch":false,"BuyerBlastOptIn":false,"BizAlertExists":false,"NeedToSubscribeToBizAlert":false,"DisplayOnPageLoad":true},
            20,
            function() { $('#BizAlertModal').modal('hide'); },
            bizAlert,
            savedSearch);

        
        $('#BizAlertModal').on('hide', function()
        {
            if ((window.BizBuySellWebSite) &&
            (window.BizBuySellWebSite.mybbs) &&
            (window.BizBuySellWebSite.mybbs.EmailAlerts) &&
            (window.BizBuySellWebSite.mybbs.EmailAlerts.Services) &&
            (window.BizBuySellWebSite.mybbs.EmailAlerts.Services.EmailAlerts) &&
