                                                            navigate = navigate || function() {};
                                                            if ((window.BizBuySellWebSite) &&
                                                            (window.BizBuySellWebSite.services) &&
                                                            (window.BizBuySellWebSite.services.DataUtilities) &&
                                                            (window.BizBuySellWebSite.services.DataUtilities.TrackBrokerEvent))
                                                            {
                                                                account = (account || 0);
                                                                actionType = (actionType || 0);

                                                                if ((account > 0) &&
                                                                (actionType > 0))
                                                                {
                                                                    window.BizBuySellWebSite.services.DataUtilities.TrackBrokerEvent(
                                                                        account,
                                                                        actionType);
                                                                }
                                                            }

                                                            navigate();
                                                        });
                                                });

                                                if (results != null)
                                                {
                                                    for (var index = 0; index < results.Brokers.length; index++)
                                                    {
                                                        ga('send', 'event', 'listing_search_results', 'broker_directory_upsell_multi_display', results.Brokers[index].BrokerProfileID);
                                                    }
                                                }
                                            });
                                    },
                                    1);
                            }

                        }
                        catch(err)
                        {
                        }
                    });
                </script>
                <!-- countyLinks -->
