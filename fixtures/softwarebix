<script type="text/javascript">
    (function ($)
    {
        $(function ()
        {
            $('a#hlMobileSideNav').sidr();

            $('a#hlMobileSideNav').on(
                'click',
                function (e)
                {
                    e.stopPropagation();
                });
                
            if(window.MyBBS)
            {
                if(!!window.MyBBS.IsUserLoggedIn)
                {
                    if(!!window.MyBBS.IsUserLoggedInAsBroker)
                    {
                        $('#sideNav_BrokerWorks').show();
                        $('#sideNav_MyBBS').hide();
                    }
                    else
                    {
                        $('#sideNav_BrokerWorks').hide();
                        $('#sideNav_MyBBS').show();
                    }
                }
                else
                {
                    $('#sideNav_BrokerWorks').hide();
                    $('#sideNav_MyBBS').hide();
                }
            }
        });

        $('.accordion').on('show hide', function (e)
        {
            $(e.target).siblings('.accordion-heading').find('.accordion-toggle i').toggleClass('icon-chevron-up icon-chevron-down');
        });
    })(jQuery);
</script>
<script type="text/javascript" src="/xcommon/scripts/jQuery/jquery.sidr.min.js"></script>

<style type="text/css">
