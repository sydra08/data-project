                            {{/RegionsList}}
                        </ul>
                    </div>
                    {{/RegionsForSearchPage}}
                </script>
                <div id="countyLinks_content">
                </div>
                <script type="text/javascript">
                    $(function()
                    {
                        var BizBuySellWebSite = window.BizBuySellWebSite;
                        try
                        {
                            if ((BizBuySellWebSite) &&
                            (BizBuySellWebSite.services) &&
                            (BizBuySellWebSite.services.DataUtilities) &&
                            (BizBuySellWebSite.services.DataUtilities.GetRegionNavigateUrls))
                                setTimeout(function()
                                    {
                                        BizBuySellWebSite.services.DataUtilities.GetRegionNavigateUrls(
                                            'bec02e74-f05c-42d2-966a-3c17ae5e2361',
                                            '<?xml version="1.0" encoding="utf-16"?>  <ListingSearchCriteria xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">    <SearchCriteriaQueryStringData>i=R6&amp;tab=eb</SearchCriteriaQueryStringData>    <HtmlEncodedSearchCriteriaQueryStringData>i=R6&amp;tab=eb</HtmlEncodedSearchCriteriaQueryStringData>    <CriteriaQueryStringParts>      <item>        <key>          <string>i</string>        </key>        <value>          <string>R6</string>        </value>      </item>      <item>        <key>          <string>tab</string>        </key>        <value>          <string>eb</string>        </value>      </item>    </CriteriaQueryStringParts>    <CriteriaInitialized>true</CriteriaInitialized>    <SearchType>EstablishedBusinesses</SearchType>    <Categories>      <BusinessType>        <BizTypeID>55</BizTypeID>        <BizType>R6</BizType>        <Description>Software</Description>        <IsSearchOnly>false</IsSearchOnly>        <HeadingBizTypeID>49</HeadingBizTypeID>        <IsHeading>false</IsHeading>        <IsMiscellaneous>false</IsMiscellaneous>        <IsAdWordsAllowed>false</IsAdWordsAllowed>        <DisplayIndex>78</DisplayIndex>        <Seo_Singular>Software</Seo_Singular>        <Seo_Plural>Software Businesses</Seo_Plural>        <Seo_Alternate1>Web Development</Seo_Alternate1>        <Seo_Alternate2>Software Development</Seo_Alternate2>        <Seo_Alternate3>Software Consulting</Seo_Alternate3>        <ItemID>55</ItemID>      </BusinessType>    </Categories>    <KeywordMatchType>Any</KeywordMatchType>    <DoNotShowListingsWithUndisclosedPrices>false</DoNotShowListingsWithUndisclosedPrices>    <OnlyShowListingsWithSellerFinancing>false</OnlyShowListingsWithSellerFinancing>    <OnlyShowListingsWithRealEstateIncluded>false</OnlyShowListingsWithRealEstateIncluded>    <ShowRelocatableListings>false</ShowRelocatableListings>    <DateListed>0001-01-01T00:00:00</DateListed>    <DateFirstApproved>0001-01-01T00:00:00</DateFirstApproved>    <DateListedType>Anytime</DateListedType>    <HomeBasedType>Include</HomeBasedType>    <AskingPriceType>None</AskingPriceType>    <ExcludeNoAskingPrice>false</ExcludeNoAskingPrice>    <IsInitialSearchRequest>false</IsInitialSearchRequest>    <SearchStarted>true</SearchStarted>    <SortFields>      <item>        <key>          <enBizopsSortFields>Default</enBizopsSortFields>        </key>        <value>          <enumSequence>Desc</enumSequence>        </value>      </item>    </SortFields>    <IsSpecialInternationalRegion>false</IsSpecialInternationalRegion>    <CurrentPageNumber>1</CurrentPageNumber>    <SearchOriginID>0</SearchOriginID>    <IsReturnToSearch>false</IsReturnToSearch>    <WhoFrom>0</WhoFrom>    <ExcludeAssetSales>false</ExcludeAssetSales>    <IsBotSearch>false</IsBotSearch>    <BrokerAccount>0</BrokerAccount>    <ReinforcementListingsBeginningPageNumber>0</ReinforcementListingsBeginningPageNumber>  </ListingSearchCriteria>',
                                            function(results)
                                            {
                                                if (results != null)
                                                {
                                                    if (results.hasOwnProperty('d'))
                                                    {
                                                        results = results.d;
                                                    }
		                                        
                                                    var countyLinks_content = $('#countyLinks_content');
		                                        
                                                    if (countyLinks_content.length > 0)
                                                    {
                                                        countyLinks_content.empty();

                                                        if (window.Mustache)
                                                        {
                                                            var countyLinkstemplate = Mustache.compile(
                                                                $('#countyLinks_template').html());

                                                            if (typeof(countyLinkstemplate) == 'function')
                                                            {
                                                                countyLinks_content
                                                                    .prepend(
                                                                        countyLinkstemplate(
                                                                        {
                                                                            RegionsForSearchPage: results,
                                                                        }));
                                                            }
                                                        }
                                                    }
                                                }
                                            });
                                    },
                                    1);

                        }
                        catch(err)
                        {
                        }

                    });
                </script>
            </div>
