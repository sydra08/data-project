    <script type="text/javascript">
        $('#hlBBSBulletinSignUp').click(function()
        {
            var txtBBSBulletinSignupEmailAddress = $('#txtBBSBulletinSignupEmailAddress');

            if (txtBBSBulletinSignupEmailAddress.length > 0)
            {
                var emailAddress = txtBBSBulletinSignupEmailAddress.val();

                if (emailAddress.length > 0)
                {
                    BBSBulletinSignupPopup.Signup(
                        emailAddress);
                }
                else
                {
                    $('#divBulletinModalError').addClass('error');

                    txtBBSBulletinSignupEmailAddress.focus();
                }
            }
        });

        var BBSBulletinSignupPopup = (function()
        {
            var bbsBulletinSignupPopup = {};

            bbsBulletinSignupPopup.Signup = function(email)
            {
                if ((window.RemoteNewsletter) &&
                (typeof(RemoteNewsletter.SignupNewsletter) == 'function'))
                {
                    RemoteNewsletter.SignupNewsletter(
                        email,
                        's1',
                        'bbs',
                        '');

                    $('#BulletinModalRequest').hide();
                    $('#hlBBSBulletinSignUp').hide();
                    $('#hlNoThanksForSignUp').hide();
                    $('#hlBBSBulletinSignupFooter').hide();
                    $('#BulletinModalResponse').show();

                    window.setTimeout("$('#BulletinModal').modal('hide');", 2000);
                }
            };

            return bbsBulletinSignupPopup;
        })();

        var ga;

        if ($.cookie('BulletinSrcAdSearch') != 1)
        {
            $('#BulletinModal').modal('show');
            $.cookie(
                'AnyModalSeenCookie', 
                '1',
                {
                    path: '/',
                    domain: '.bizbuysell.com'
                });
            $.cookie(
                'BulletinSrcAdSearch', 
                '1',
                {
                    path: '/',
                    domain: '.bizbuysell.com',
                    expires: 90,
                });
            (function()
            {
                ga('send', 'event', 'listing_search_results', 'buyerbulletin', 'show');
            })();
        }
    </script>
