<script type="text/javascript">
    
    $(function()
    {
        ListingProfile.EmailPattern = /^[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?\.)+[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?$/;
        ListingProfile.PhonePattern = /(^1?[- .]?(\([2-9]\d{2}\)|[2-9]\d{2})[- .]?(\d{3})[- .]?(\d{4})(\s*e?[Xx]t?(ension)?[.:]*\s*\d{1,5})?$|^\+?\d{1,3}[ -]?[0-9 -]{9,14}(\s*e?[Xx]t?(ension)?[.:]*\s*\d{1,5})?$)/;

        ListingProfile.ValidateModal = function(listNumber, requireName, requirePhone)
        {
            listNumber = (listNumber || 0);
            requirePhone = !!requirePhone;

            

            var errors = [];

            var hlSubmit = $('#hlContactSellerModal');

            disableButton(hlSubmit);

            var form = $('#contactSellerFormModal');

            var txtEmail = form.find('#txtEmailModal');
            var txtName = form.find('#txtNameModal');
            var txtPhone = form.find('#txtPhoneModal');
            var txtZip = form.find('#txtZipModal');
            var txtInvest = form.find('#txtInvestModal');
            var txtMonths = form.find('#txtMonthsModal');
            var txtMessage = form.find('#txtMessageModal');
            var chkNewsletter = form.find('#chkNewsletterModal');
            var financingFranchiseID = parseInt(form.find('.financingFranchiseModal:checked').val(), 10) || 0;

            form.find('.error').removeClass('error');

            if (!txtEmail.val() || !ListingProfile.EmailPattern.test(txtEmail.val()))
            {
                errors.push(ClientSideError.RequiredFieldFormatError(txtEmail));
                $('#divEmailModal').addClass('error');
            }

            $('#plcFullNameRequiredErrorMessageModal').hide();

            var name = (txtName.val() || '').trim();
            var fullNamedProvided = name.indexOf(' ') > 0;

            if (name == '' || 
                (financingFranchiseID > 0 && !fullNamedProvided))
            {
                errors.push(ClientSideError.RequiredFieldFormatError(txtName));
                $('#divNameModal').addClass('error');

                if (financingFranchiseID > 0 && !fullNamedProvided)
                {
                    $('#plcFullNameRequiredErrorMessageModal').show();
                }
            }
            
            
            if (requirePhone && (!txtPhone.val() || !ListingProfile.PhonePattern.test(txtPhone.val())))
            {
                errors.push(ClientSideError.RequiredFieldFormatError(txtPhone));
                $('#divPhoneModal').addClass('error');
            }
            else if (!requirePhone && txtPhone.val() && !ListingProfile.PhonePattern.test(txtPhone.val()))
            {
                errors.push(ClientSideError.RequiredFieldFormatError(txtPhone));
                $('#divPhoneModal').addClass('error');
            }
            
            
            if (!txtMessage.val())
            {
                errors.push(ClientSideError.RequiredField(txtMessage));
                $('#divMessageModal').addClass('error');
            }

            if (errors.length == 0)
            {
                hlSubmit.addClass('loading');

                if (financingFranchiseID > 0)
                {
                    ga('send', 'event', 'bfslead', 'checkboxchecked', financingFranchiseID);
                }
                
                window.BizBuySellWebSite.listings.Services.ListingUtilities.ContactSeller(
                    listNumber,
                    0,
                    txtEmail.val(),
                    name,
                    name,
                    txtPhone.val(),
                    txtPhone.val(),
                    txtZip.val() || '',
                    txtInvest.val() || '',
                    txtMonths.val() || '',
                    txtMessage.val(),
                    chkNewsletter.is(':checked'),
                    financingFranchiseID,
                    false,
                    true,
                    true,
                    false,
                    true,
                    function(result)
                    {
	                    result = result || { EmailSent: false, ReviewQEmailNumber: 0}; 
	                    
						if (!!result.EmailSent)
						{
						    
						    $('#contactSeller_' + listNumber).unbind();
						    $('#contactSeller_' + listNumber).addClass('contacted');
						    $('#contactSeller_' + listNumber).html('<span class="visible-desktop visible-tablet">Seller Contacted</span><i class="custom-check hidden-desktop hidden-tablet"></i>');
                            
						    $('#contactModal').modal('hide');
						    enableButton(hlSubmit, listNumber, requireName, requirePhone);

						    ga('send', 'event', 'listing_search_results', 'contact_popup', 'sent');
                            
                        }
                        else
                        {
						    enableButton(hlSubmit, listNumber, requireName, requirePhone);
                        }
                    },
                    function()
                    {
                        enableButton(hlSubmit, listNumber, requireName, requirePhone);
                    });
            }
            else
            {
                enableButton(hlSubmit, listNumber, requireName, requirePhone);
                TrackErrors(errors, 'Contact Seller');
            }
        };

        function disableButton(
            hlSubmit)
        {
            hlSubmit.addClass('disabled');
            hlSubmit.unbind('click');
        }

        function enableButton(
            hlSubmit,
            listNumber,
            needsName,
            needsPhone)
        {
            hlSubmit.removeClass('disabled');
            
            hlSubmit.bind(
                'click', 
                function()
                {
                    ListingProfile.ValidateModal(
                        listNumber,
                        needsName,
                        needsPhone);
                });

            hlSubmit.removeClass('loading');
        }
    });
</script>

            </div>
        </div>
        <div aria-hidden="false" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" class="modal hide fade in" id="BizAlertModal" style="display: none;">
          <div data-dismiss="modal" class="close-modal"></div>
          <div class="modal-body">
            <legend>Be First to Know with BizAlerts</legend>
            <p><b>Get free alerts via email when new businesses are listed matching your <span class="redtext">Software</span> search criteria.</b></p>
