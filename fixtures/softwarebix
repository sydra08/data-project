        { hitType: 'event', eventCategory: 'listing_search_results', eventAction: 'premmember_popup', eventLabel: 'redirectToVideos' });
        
        $('#PremMemberModal .recsearch').trackNavigate(
        { hitType: 'event', eventCategory: 'listing_search_results', eventAction: 'premmember_popup', eventLabel: 'redirectToRecSearch' });

        $('#PremMemberModal .close-modal').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'premmember_popup', 'close');
        });
        
