<script type="text/javascript">
    var TopNavAlert = (function()
    {
        var topNavAlert = {};

        topNavAlert.Initialize = function (
            options)
        {
            options = options || {
                AlertClosedCookieName: '',
                HideAlertBodyClass: '',
                AlertClientID: '',
                HideAlertLinkClientID: '',
                GAEventCategory: ''
            };

            $('#' + options.AlertClientID + ' a').hover(function ()
            {
                $(this).css({
                    'text-decoration': 'none',
                    'color': '#000'
                });
            }, function ()
            {
                $(this).css({
                    'text-decoration': 'none',
                    'color': '#505050'
                });
            });

            $('#' + options.AlertClientID + ' a b').hover(function ()
            {
                $(this).css(
                    'text-decoration',
                    'underline');
            }, function ()
            {
                $(this).css(
                    'text-decoration',
                    'none');
            });

            function cookieAlert()
            {
                $.cookie(
                    options.AlertClosedCookieName,
                    '1',
                    {
                        path: '/',
                        domain: '.bizbuysell.com',
                        expires: 5 * 365
                    });
            }

            if (!$('body').hasClass(options.HideAlertBodyClass))
            {
                var userClosedAlert = ($.cookie(options.AlertClosedCookieName) || '');

                if (userClosedAlert == '0' || userClosedAlert == '')
                {
                    $('#' + options.AlertClientID).fadeIn('slow', function()
                    {
                        $('#' + options.AlertClientID).css('display', 'block');
                    });
                }

                $('#' + options.HideAlertLinkClientID).click(
                    function ()
                    {
                        ga('send', 'event', options.GAEventCategory, 'alertBarClose', '/listings/Search.aspx');

                        cookieAlert();

                        $('#' + options.AlertClientID).fadeOut('slow');
                    });

                $('#' + options.AlertLinkClientID).executeBeforeNavigation(
                    function()
                    {
                        ga('send', 'event', options.GAEventCategory, 'alertBar', '/listings/Search.aspx');

                        cookieAlert();
                    });
            }
        };
	    return topNavAlert;
    })();
</script>
  
<script type="text/javascript">
    if ((window.Users) &&
        (window.BizBuySellWebSite) &&
        (BizBuySellWebSite.users) &&
        (BizBuySellWebSite.users.Services) &&
        (BizBuySellWebSite.users.Services.Users))
    {
        Users.UserUtilities = BizBuySellWebSite.users.Services.Users;
    }
</script>
        

        
