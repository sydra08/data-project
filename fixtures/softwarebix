<script type="text/javascript">
    $('.contactSeller').click(function(e)
    {
        e.stopPropagation();
        e.preventDefault();

        $('#cmBrokerPhoto').attr('src', '/xcommon/images/broker/nophoto.jpg');
        $('#divEmailModal').removeClass('error');
        $('#divNameModal').removeClass('error');
        $('#divPhoneModal').removeClass('error');
        $('#divMessageModal').removeClass('error');

        var $this = $(this);

        var listNumber = (parseInt($this.data('listnumber'), 10) || 0);

        var needsName = !!$this.data('needsname');
        var needsPhone = !!$this.data('needsphone');

        $("#hlContactSellerModal")
            .unbind('click')
            .removeClass('disabled')
            .click(
                function()
                {
                    ListingProfile.ValidateModal(
                        listNumber,
                        needsName,
                        needsPhone);
                });

        $('#contactModalHeadline').text('Contact Seller for: ' + $this.data('headline'));

        //See which controls we need and which we don't
        if ($this.data('showzip') == true) 
        {
            $('#divZipModal').show();
        }
        else 
        {
            $('#divZipModal').hide();
        }

        if ($this.data('showinvest') == true) 
        {
            $('#investAmountModal').show();
        }
        else 
        {
            $('#investAmountModal').hide();
        }

        if ($this.data('showtimeframe') == true)
        {
            $('#divMonthsModal').show();
        }
        else 
        {
            $('#divMonthsModal').hide();
        }

        if ($this.data('brokerprofileid')) 
        {
            $('.modalBroker').show();

            $('#cmBrokerName')
                .text(
                    $this.data('brokername'))
                .attr('href', $this.data('brokerlink'))
                .data(
                    'brokeraccount',
                    $this.data('brokeraccount'));

            $('#cmBrokerCompanyName').text(
                $this.data('brokercompany'));

            $('#cmBrokerPhonenumber').text(
                $this.data('brokerphone'));

            $('#cmBrokerPhoto').attr('src', $this.data('brokerphoto'));
        }
        else
        {
            $('.modalBroker').hide();
        }

        $('#contactModal').modal('show');
    });
</script>

