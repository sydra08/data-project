<script type="text/javascript">
    $('#SaveSearchButton').click(function()
    {
        var saveSearchRegistrationType = 60;

        if (window.MyBBS)
        {
            var userLoggedIn = !!MyBBS.IsUserLoggedIn;
            var savedSearchName = ($('#inputNameForSearch').val() || '');
            if (savedSearchName == '')
            {
                $('#divNameForSearch').addClass('error');
                $('#inputNameForSearch').focus();
            }
            else if (!userLoggedIn)
            {
                $('#divNameForSearch').removeClass('error');

                if (typeof(MyBBS.ToggleSignInPopup) == 'function')
                {
                    $(document).bind(
                        'OnMyBBSLoginSuccessful',
                        function(
                            evt,
                            registrationType)
                        {
                            registrationType = (registrationType || 0);

                            switch (registrationType)
                            {
                                case saveSearchRegistrationType:
                                {
                                    if ((window.Listings) && (typeof(Listings.SaveSearch) == 'function'))
                                    {
                                        Listings.SaveSearch(
                                            savedSearchName,
                                            'i=R6&tab=eb',
                                            function()
                                            {
                                                $('#SaveSearchModal').modal('hide');
                                                $('#SavedSearchModal').modal('show');
                                              
                                                $('#SavedSearchModal').on('hidden', function ()
                                                {
                                                    window.location.reload();
                                                });
                                         });

                                     }
                                }
                            }
                        });

                    MyBBS.ToggleSignInPopup(
                        {
                            RegistrationType: saveSearchRegistrationType,
                            CustomLoginHeading: 'Save this Search',
                            CustomLoginSubHeading: ('Log in to save: <b>' + savedSearchName + '</b>'),
                            OnRegistrationSuccessfulRedirectUrl: ('/listings/Search.aspx?i*R6^tab*eb^l_ssn*' + savedSearchName)
                        });
                }
            }
            else
            {
                if ((window.Listings) && (typeof(Listings.SaveSearch) == 'function'))
                {
                    Listings.SaveSearch(savedSearchName, 'i=R6&tab=eb');
                }

                $('#linkSaveSearchModal').hide();
                $('#lableSavedSearch').show();
                $('#SaveSearchModal').modal('hide');
                $('#SavedSearchModal').modal('show');
            }
        }
    });
</script>

        

