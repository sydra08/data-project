    <script type="text/javascript">
        var ga;

        $('#refine').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'refinesearch', 'open');
        });

        $('#moreSearch').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'refinesearch', 'more');
        });

        $('#lessSearch').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'refinesearch', 'less');
        });

        $('#linkSaveSearchModal').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'savesearch', 'open');
        });

        $('#SaveSearchButton').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'savesearch', 'save');
        });

        $('#SaveSearchModal .close-modal').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'savesearch', 'close');
        });

        $('#SaveSearchModal [data-dismiss=modal]:not(.close-modal)').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'savesearch', 'dismiss');
        });

        $('#bizAlert_Save').trackNavigate(
        { hitType: 'event', eventCategory: 'listing_search_results', eventAction: 'bizalert', eventLabel: 'click' });

        $('.contactSeller:not(.contacted)').click(function()
        {
            //Need to check again to make sure it was not contacted while on the same page.
            if (!$(this).hasClass('contacted'))
            {
                ga('send', 'event', 'listing_search_results', 'contact_popup', 'open');
            }
        });

        $('#BizAlertModal .close-modal').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'bizalert_popup', 'close');
        });

        $('#BizAlertModal [data-dismiss=modal]:not(.close-modal)').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'bizalert_popup', 'dismiss');
        });

        $('#bizAlert_FinalSubmit').click(function()
        {
            ga('send', 'event', 'listing_search_results', 'bizalert_popup', 'save');
        });
        
        $('#PremMemberModal .modal-footer a').trackNavigate(
        { hitType: 'event', eventCategory: 'listing_search_results', eventAction: 'premmember_popup', eventLabel: 'redirectToPremSignup' });
        
