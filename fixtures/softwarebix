<script type="text/javascript">
    var frannieBirdIndex = 0;
        
    var FrannieBirdize = function()
    {
        $('.listingResult:not(.indexed)')
            .each(function()
            {
                var franchiseContainer = [];

                switch (++frannieBirdIndex)
                {
                    case 6:
                    {
                        franchiseContainer = $('#FranchiseRowContainer1');
                                
                        break;
                    }
                            
                    case 19:
                    {
                        franchiseContainer = $('#FranchiseRowContainer2');
                            
                        break;
                    }
                            
                    case 32:
                    {
                        franchiseContainer = $('#FranchiseRowContainer3');
                            
                        break;
                    }
                            
                    case 45:
                    {
                        franchiseContainer = $('#FranchiseRowContainer4');
                            
                        break;
                    }
                }
                    
                var $this = $(this);

                if (franchiseContainer.length > 0)
                {
                    $this.before(franchiseContainer);

                    franchiseContainer.addClass('index_' + frannieBirdIndex);

                    if ($this.hasClass('featured'))
                    {
                        franchiseContainer.children('a').toggleClass('basic featured');
                    }

                    if (!$this.hasClass('diamond'))
                    {
                        franchiseContainer.show();
                    }
                }

                $this.addClass('indexed');
            });
    };
</script>

<script type="text/javascript">
    
    if (Ad1 == true)
        $('#Ad1').show();
    if (Ad2 == true)
        $('#Ad2').show();
    if (Ad3 == true)
        $('#Ad3').show();
    if (Ad4 == true)
        $('#Ad4').show();
    
    var ga;
                            
    //this is for the results that are loaded with the page
    $('a.listingResult').each(function()
    {
        var listNumber = parseInt(
            $(this).data('listnumber'),
            10) || 0;
                                    
        if (listNumber > 0)
        {
            $(this).executeBeforeNavigation(
                function(navigate)
                {
                    navigate = navigate || function() {};
                                        
                    if ((window.Listings))
                    {
                        if (typeof(Listings.TrackListingAction) == 'function')
                        {
                            Listings.TrackListingAction.call(
                                this,
                                listNumber,
                                170,
                                0,
                                true);
                        }
                    }

                    navigate();
                });
        }
    });

    $('.saveFavorite').click(function(event)
    {
        event.preventDefault();
        event.stopPropagation();

        var listNumber = parseInt(
            $(this).data('listnumber'),
            10) || 0;

        if (listNumber > 0)
        {
            if ((!$(this).hasClass('saved')) && window.Listings &&
                typeof(Listings.AddListingToWatchList) == 'function')
            {
                Listings.AddListingToWatchList.call(
                    this,
                    listNumber,
                    window.location.href,
                    function()
                    {
                        $('#saveFavorite_' + listNumber).removeClass('processing');

                        $('#saveFavorite_' + listNumber).addClass('saved').addClass('processing').attr('title', 'Remove from your saved listings');
                        ga('send', 'event', 'listing_search_results', 'action', 'watch');
                    });
            }
            else if ($(this).hasClass('saved') && window.Listings &&
                typeof(Listings.RemoveListingFromWatchListWithCallback) == 'function')
            {
                $('#saveFavorite_' + listNumber).removeClass('saved').addClass('processing').attr('title', 'Add to your saved listings');
                ga('send', 'event', 'listing_search_results', 'action', 'unwatched');

                Listings.RemoveListingFromWatchListWithCallback.call(
                    this,
                    listNumber,
                    function()
                    {
                        $('#saveFavorite_' + listNumber).removeClass('processing');
                    });
            }
        }
    });
                            
    $('span.hotProperty').each(function () { if ($(this).siblings().length == 0) { $(this).css('border-right', 'none'); } });
</script>
